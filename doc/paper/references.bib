}

@article{Loudon1969AMRRD,
	title = {Aerial {Transmission} of {Mycobacteria}},
	volume = {100},
	issn = {0003-0805},
	doi = {10.1164/arrd.1969.100.2.165},
	abstract = {The Henderson apparatus was used to expose mice to aerosols of Mycobacterium tuberculosis (H37Rv), M. kansasii (strain P1), another photochromogenic mycobacterium (P8), and two strains of nonphotochromogenic mycobacteria (T-39 and P2). Organisms were enumerated in the bacterial slurries before and after nebulization in a Collison nebulizer, in air samples from the equipment during nebulization, and in the lungs of animals killed at various intervals after exposure. A Goldberg rotating drum was used to test the ability of the organisms to survive aerial suspension.

The mycobacteria withstood aerosolization and aerial suspension about equally well and were found in equivalent numbers in lungs from animals killed immediately after exposure. The numbers of organisms in lungs of animals killed three days later were approximately the same, but thereafter lung clearance rates varied considerably. By the eleventh day, the atypical mycobacteria had been reduced in numbers (with the exception of the photochromogenic organism, P8), but the typical tubercle bacilli had multiplied.

These observations show that the difference between M. tuberculosis and atypical mycobacteria with respect to transmission of disease resides in the host-parasite relationship after implantation rather than in a variation in capacity for aerial transmission or deposition after inhalation.},
	number = {2},
	urldate = {2022-11-25},
	journal = {American Review of Respiratory Disease},
	author = {Loudon, Robert G. and Bumgarner, Linda R. and Lacy, Jacqueline and Coffman, Gary K.},
	year = {1969},
	pages = {165--171},
	file = {Loudon et al_1969_Aerial Transmission of Mycobacteria.pdf:/Users/nb22i345/Zotero/storage/Y7P96GMB/Loudon et al_1969_Aerial Transmission of Mycobacteria.pdf:application/pdf},
}

