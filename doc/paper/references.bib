	journal = {PLOS ONE},
	author = {Hui, David S. and Chow, Benny K. and Chu, Leo and Ng, Susanna S. and Lee, Nelson and Gin, Tony and Chan, Matthew T. V.},
	year = {2012},
	keywords = {Coughing, Influenza, Lasers, Light, Light scattering, Nosocomial infections, Respiratory infections, Surgical and invasive medical procedures},
	pages = {e50845},
	file = {Hui et al_2012_Exhaled Air Dispersion during Coughing with and without Wearing a Surgical or.pdf:/Users/nb22i345/Zotero/storage/KBURNDHK/Hui et al_2012_Exhaled Air Dispersion during Coughing with and without Wearing a Surgical or.pdf:application/pdf},
